{
  "extends": [
    "react-app",
    "plugin:prettier/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "ignorePatterns": ["**/node_modules", "**/public", "**/build"]
}
